{% if space.twitter_url != empty || space.facebook_url != empty || space.linkedin_url != empty %}

  <ul class="inline-list">
    {% if space.twitter_url != empty %}
      <li class="inline-list__item">
        <a href="{{ space.twitter_url }}" target="_blank" class="icon">
          <svg viewBox="0 0 32 32">
            <use class="icon__icon" xlink:href="#twitter-icon"></use>
            <use class="icon__mask" xlink:href="#twitter-mask"></use>
          </svg>
        </a>
      </li>
    {% endif %}

    {% if space.facebook_url != empty %}
      <li class="inline-list__item">
        <a href="{{ space.facebook_url }}" target="_blank" class="icon">
          <svg viewBox="0 0 32 32">
            <use class="icon__icon" xlink:href="#facebook-icon"></use>
            <use class="icon__mask" xlink:href="#facebook-mask"></use>
          </svg>
        </a>
      </li>
    {% endif %}

    {% if space.linkedin_url != empty %}
      <li class="inline-list__item">
        <a href="{{ space.linkedin_url }}" target="_blank" class="icon">
          <svg viewBox="0 0 32 32">
            <use class="icon__icon" xlink:href="#linkedin-icon"></use>
            <use class="icon__mask" xlink:href="#linkedin-mask"></use>
          </svg>
        </a>
      </li>
    {% endif %}
  </ul>

{% endif %}
